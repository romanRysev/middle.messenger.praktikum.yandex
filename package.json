    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "eslint": "8.21.0",
    "express": "4.18.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "20.0.0",
